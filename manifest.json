{
"manifest_version": 2,
    "name": "Miles Per Year",
    "description": "Make the used car search easier by adding a \"miles per year\" field on inidividual listings.",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "*://*.autotrader.com/cars-for-sale/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}